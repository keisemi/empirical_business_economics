status collapsed

\begin_layout Plain Layout

https://web.archive.org/web/20171116012208/http://faculty.wcas.northwestern.edu/
\backslash
textasciitilde ane686/supplements/Ras
\backslash
_guide
\backslash
_appendix.pdf
\end_layout

\end_inset

).
\end_layout

\end_inset

を参照されたい。
 
\end_layout

\begin_layout Itemize
ここで、行列の中身の微分について 
\begin_inset Formula 
\begin{align*}
\frac{\partial s_{jt}}{\partial\delta_{jt}} & =\frac{1}{R}\sum_{i=1}^{R}\frac{\partial s_{rjt}}{\partial\delta_{jt}}=\frac{1}{R}\sum_{i=1}^{R}s_{ijt}(1-s_{ijt}),\\
\frac{\partial s_{jt}}{\partial\delta_{mt}} & =\frac{1}{R}\sum_{i=1}^{R}\frac{\partial s_{rjt}}{\partial\delta_{mt}}=-\frac{1}{R}\sum_{i=1}^{R}s_{ijt}s_{imt}.
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
また、 
\begin_inset Formula 
\begin{align*}
\frac{\partial s_{jt}}{\partial\sigma^{k}} & =\frac{1}{R}\sum_{i=1}^{R}\frac{\partial s_{rjt}}{\partial\sigma^{k}}=\frac{1}{R}\sum_{i=1}^{R}s_{ijt}\left(x_{jt}^{k}v_{i}^{k}-\sum_{m=1}^{J}x_{mt}^{k}v_{i}^{k}s_{imt}\right).
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
漸近分散(Asymptotic Variance)
\end_layout

\begin_layout Itemize
非線形GMMの漸近分散は以下のように与えられる。
 詳細については、末石(2015)やHayashi (2000)などを参照されたい。
 
\begin_inset Formula 
\[
\sqrt{N}(\hat{\theta}-\theta)\overset{d}{\rightarrow}N\left(0,(G^{\prime}WG)^{-1}G^{\prime}W\Omega WG(G^{\prime}WG)^{-1}\right).
\]

\end_inset


\end_layout

\begin_layout Itemize
ここで、 
\begin_inset Formula 
\[
\underbrace{\Omega}_{(L\times L)}=E[\xi_{jt}z_{jt}z_{jt}^{\prime}\xi_{jt}].
\]

\end_inset

そして 
\begin_inset Formula 
\begin{align*}
\underbrace{G}_{(L\times K)} & =E\left[z_{jt}\frac{\partial\xi_{jt}(\theta)}{\partial\theta}\right]\\
 & =E\left[z_{jt}\frac{\partial(\delta_{jt}(\theta_{2})-x_{jt}^{\prime}\theta_{1})}{\partial\theta}\right]\\
 & =\left(\underbrace{E\left[z_{jt}(-x_{jt}^{\prime})\right]}_{(L\times K_{1})},\underbrace{E\left[z_{jt}\frac{\partial\delta_{jt}(\theta_{2})}{\partial\theta}\right]}_{(L\times K_{2})}\right).
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
この結果から、推定パラメター
\begin_inset Formula $\hat{\theta}$
\end_inset

は漸近的に
\begin_inset Formula $\hat{\theta}\sim N(\theta,\Sigma/N)$
\end_inset

と近似できる。
この結果に基づいてパラメタの漸近標準誤差を計算することができる。
 具体的には、漸近標準誤差は
\begin_inset Formula $\sqrt{diag(\Sigma)/N}$
\end_inset

となる。
なお、
\begin_inset Formula $diag(\Sigma)$
\end_inset

は行列
\begin_inset Formula $\Sigma$
\end_inset

の対角要素を示す。
 
\end_layout

\begin_layout Subsection
漸近分散の推定
\end_layout

\begin_layout Itemize
漸近分散の推定は、期待値を対応する標本平均で置き換えることとなる。
これを標本対応(sample analogue)と呼ぶ。
 
\end_layout

\begin_layout Itemize
すなわち、 
\begin_inset Formula 
\begin{align*}
\underbrace{\hat{\Omega}}_{(L\times L)} & =\frac{1}{N}\sum_{j,t}\hat{\xi}_{jt}z_{jt}z_{jt}^{\prime}\hat{\xi}_{jt}.
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
ここで、
\begin_inset Formula $\hat{\xi}_{jt}$
\end_inset

は推定値に基づく残差であり、
\begin_inset Formula $\hat{\xi}_{jt}=\delta_{jt}(\hat{\theta}_{2})-x_{jt}^{\prime}\hat{\theta}_{1}$
\end_inset

である。
 
\end_layout

\begin_layout Itemize
また、
\begin_inset Formula $\hat{\xi}=(\hat{\xi}_{1},\cdots,\hat{\xi}_{N})^{\prime}$
\end_inset

 は 
\begin_inset Formula $(N\times1$
\end_inset

) ベクトル。
 
\end_layout

\end_deeper
\begin_layout Itemize
同様にして、
\begin_inset Formula $G$
\end_inset

の標本対応は 
\begin_inset Formula 
\begin{align*}
\hat{G} & =\left(\frac{1}{N}\sum_{j,t}z_{jt}\frac{\partial\delta_{jt}(\theta_{2})}{\partial\theta},\,\,\frac{1}{N}\sum_{j,t}z_{jt}(-x_{jt}^{\prime})\right)\\
 & =\frac{1}{N}Z^{\prime}\left(\underbrace{-X}_{(N\times K_{1})},\,\,\underbrace{D\delta(\theta_{2})}_{(N\times K_{2})}\right).
\end{align*}

\end_inset


\end_layout

